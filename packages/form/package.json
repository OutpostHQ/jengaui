{
  "name": "@jengaui/form",
  "version": "0.2.0",
  "description": "Jenga UI Form component in React",
  "keywords": [
    "form",
    "react",
    "Jenga UI",
    "ui",
    "clickable",
    "tabbable"
  ],
  "bugs": "https://github.com/Outposthq/jengaui/issues",
  "repository": "Outposthq/jengaui",
  "license": "MIT",
  "main": "./dist/index.js",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "files": [
    "dist/**"
  ],
  "scripts": {
    "build": "JSX=1 tsup src/index.ts --dts --minify --treeshake --splitting",
    "clean": "rm -rf .turbo && rm -rf node_modules && rm -rf dist",
    "dev": "tsup src/index.ts --minify --treeshake --format esm,cjs --watch --dts --external react",
    "lint": "TIMING=1 eslint src/**/*.ts* --fix"
  },
  "dependencies": {
    "@jengaui/alert": "workspace:0.2.0",
    "@jengaui/content": "workspace:0.2.0",
    "@jengaui/layout": "workspace:0.2.0",
    "@jengaui/providers": "workspace:0.2.0",
    "@jengaui/tooltip": "workspace:0.2.0",
    "@jengaui/utils": "workspace:0.2.0",
    "@react-spectrum/utils": "^3.7.4",
    "@react-stately/collections": "^3.4.4",
    "@react-types/shared": "^3.15.0",
    "email-validator": "^2.0.4",
    "tastycss": "^0.10.0",
    "valid-url": "^1.0.9"
  },
  "devDependencies": {
    "@ant-design/icons": "^4.7.0",
    "@jengaui/tsconfig": "workspace:0.1.1",
    "@types/react": "^18.0.21",
    "@types/react-dom": "^18.0.6",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "styled-components": "^5.3.6",
    "tsup": "^6.3.0",
    "typescript": "^4.8.4"
  },
  "peerDependencies": {
    "react": ">= 18.2.0",
    "react-dom": ">= 18.2.0",
    "styled-components": ">= 5.3.6"
  },
  "publishConfig": {
    "access": "public"
  }
}
