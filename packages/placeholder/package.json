{
  "name": "@jengaui/placeholder",
  "version": "0.2.1",
  "description": "Jenga UI Placeholder component in React",
  "keywords": [
    "placeholder",
    "skeleton",
    "loader",
    "react",
    "Jenga UI",
    "ui"
  ],
  "homepage": "https://github.com/Outposthq/jengaui",
  "bugs": {
    "url": "https://github.com/Outposthq/jengaui/issues"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Outposthq/jengaui.git"
  },
  "license": "MIT",
  "sideEffects": false,
  "source": "src/index.ts",
  "main": "dist/main.js",
  "module": "dist/module.js",
  "types": "dist/types.d.ts",
  "files": [
    "dist",
    "src"
  ],
  "scripts": {
