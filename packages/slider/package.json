{
  "name": "@jengaui/slider",
  "version": "0.1.5",
  "license": "MIT",
  "description": "Jenga UI Slider component in React",
  "keywords": [
    "slider",
    "range slider",
    "react",
    "Jenga UI",
    "ui",
    "clickable",
    "tabbable"
  ],
  "main": "./dist/index.js",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "files": [
    "dist/**"
  ],
  "sideEffects": false,
  "scripts": {
    "build": "JSX=1 tsup --minify --treeshake",
    "dev": "tsup src/index.ts --minify --treeshake --format esm,cjs --watch --dts --external react",
    "lint": "TIMING=1 eslint src/**/*.ts* --fix",
    "clean": "rm -rf .turbo && rm -rf node_modules && rm -rf dist"
  },
  "publishConfig": {
    "access": "public"
  },
  "peerDependencies": {
    "react": ">= 17.0.0",
    "react-dom": ">= 17.0.0"
  },
  "devDependencies": {
    "@ant-design/icons": "^4.7.0",
    "@jengaui/tsconfig": "workspace:*",
    "@types/react": "^17.0.38",
    "@types/react-dom": "^17.0.11",
    "csstype": "3.1.1",
    "tsup": "^6.2.3",
    "typescript": "^4.5.4"
  },
  "dependencies": {
    "@internationalized/number": "^3.1.1",
    "@jengaui/core": "workspace:*",
    "@jengaui/layout": "workspace:*",
    "@jengaui/providers": "workspace:*",
    "@jengaui/tooltip": "workspace:*",
    "@jengaui/utils": "workspace:*",
    "@react-aria/focus": "^3.5.0",
    "@react-aria/i18n": "^3.5.1",
    "@react-aria/interactions": "^3.7.0",
    "@react-aria/slider": "^3.2.1",
    "@react-aria/utils": "^3.11.0",
    "@react-aria/visually-hidden": "^3.3.1",
    "@react-stately/slider": "^3.2.1",
    "@react-types/shared": "^3.10.1",
    "react-aria": "^3.19.0",
    "tastycss": "^0.8.9"
  },
  "homepage": "https://github.com/Outposthq/jengaui",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Outposthq/jengaui.git"
  },
  "bugs": {
    "url": "https://github.com/Outposthq/jengaui/issues"
  }
}
