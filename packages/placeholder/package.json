{
  "name": "@jengaui/placeholder",
  "version": "0.2.1",
  "description": "Jenga UI Placeholder component in React",
  "keywords": [
    "placeholder",
    "skeleton",
    "loader",
    "react",
    "Jenga UI",
    "ui"
  ],
  "homepage": "https://github.com/Outposthq/jengaui",
  "bugs": {
    "url": "https://github.com/Outposthq/jengaui/issues"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Outposthq/jengaui.git"
  },
  "license": "MIT",
  "sideEffects": false,
  "main": "./dist/index.js",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "files": [
    "dist/**"
  ],
  "scripts": {
    "build": "JSX=1 tsup src/index.ts --config ./tsup.config.ts",
    "clean": "rm -rf .turbo && rm -rf node_modules && rm -rf dist",
    "dev": "tsup src/index.ts --minify --treeshake --format esm,cjs --watch --dts --external react",
    "lint": "TIMING=1 eslint src/**/*.ts* --fix"
  },
  "dependencies": {
    "tastycss": "^0.9.1"
  },
  "devDependencies": {
    "@jengaui/tsconfig": "workspace:0.1.2",
    "@types/react": "^17.0.50",
    "@types/react-dom": "^17.0.17",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "styled-components": "^5.3.6",
    "tsup": "^6.2.3",
    "typescript": "^4.8.4"
  },
  "peerDependencies": {
    "react": ">= 18.2.0",
    "react-dom": ">= 18.2.0",
    "styled-components": ">= 5.3.6"
  },
  "publishConfig": {
    "access": "public"
  }
}