{
  "name": "@jengaui/menu",
  "version": "0.1.5",
  "license": "MIT",
  "description": "Jenga UI Menu component in React",
  "keywords": [
    "menu",
    "react",
    "Jenga UI",
    "ui"
  ],
  "main": "./dist/index.js",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "files": [
    "dist/**"
  ],
  "sideEffects": false,
  "scripts": {
    "build": "JSX=1 tsup --minify --treeshake",
    "dev": "tsup src/index.ts --minify --treeshake --format esm,cjs --watch --dts --external react",
    "lint": "TIMING=1 eslint src/**/*.ts* --fix",
    "clean": "rm -rf .turbo && rm -rf node_modules && rm -rf dist"
  },
  "publishConfig": {
    "access": "public"
  },
  "peerDependencies": {
    "react": ">= 17.0.0",
    "react-dom": ">= 17.0.0"
  },
  "devDependencies": {
    "@ant-design/icons": "^4.7.0",
    "@jengaui/tsconfig": "*",
    "@types/react": "^17.0.38",
    "@types/react-dom": "^17.0.11",
    "csstype": "3.1.1",
    "tsup": "^6.2.3",
    "typescript": "^4.5.4"
  },
  "dependencies": {
    "@jengaui/button": "^0.1.5",
    "@jengaui/content": "^0.1.5",
    "@jengaui/layout": "^0.1.5",
    "@jengaui/modal": "^0.1.5",
    "@jengaui/utils": "^0.1.5",
    "@react-aria/focus": "^3.5.0",
    "@react-aria/interactions": "^3.7.0",
    "@react-aria/menu": "^3.4.3",
    "@react-aria/overlays": "^3.7.5",
    "@react-aria/utils": "^3.11.0",
    "@react-spectrum/utils": "^3.6.3",
    "@react-stately/collections": "^3.3.4",
    "@react-stately/menu": "^3.3.1",
    "@react-stately/tree": "^3.3.1",
    "@react-types/menu": "^3.5.2",
    "@react-types/overlays": "^3.5.5",
    "@react-types/shared": "^3.10.1",
    "tastycss": "^0.8.9"
  },
  "homepage": "https://github.com/Outposthq/jengaui",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Outposthq/jengaui.git"
  },
  "bugs": {
    "url": "https://github.com/Outposthq/jengaui/issues"
  }
}
