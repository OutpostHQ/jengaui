{
  "name": "@jengaui/skeleton",
  "version": "0.2.0",
  "description": "Jenga UI Skeleton component in React",
  "keywords": [
    "skeleton",
    "placeholder",
    "loader",
    "react",
    "Jenga UI",
    "ui"
  ],
  "homepage": "https://github.com/Outposthq/jengaui",
  "bugs": {
    "url": "https://github.com/Outposthq/jengaui/issues"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Outposthq/jengaui.git"
  },
  "license": "MIT",
  "sideEffects": false,
  "main": "./dist/index.js",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "files": [
    "dist/**"
  ],
  "scripts": {
    "build": "JSX=1 tsup src/index.ts --dts --minify --treeshake --splitting",
    "clean": "rm -rf .turbo && rm -rf node_modules && rm -rf dist",
    "dev": "tsup src/index.ts --minify --treeshake --format esm,cjs --watch --dts --external react",
    "lint": "TIMING=1 eslint src/**/*.ts* --fix"
  },
  "dependencies": {
    "@jengaui/layout": "workspace:0.2.0",
    "@jengaui/placeholder": "workspace:0.2.0",
    "tastycss": "^0.10.0"
  },
  "devDependencies": {
    "@jengaui/tsconfig": "workspace:0.1.1",
    "@types/react": "^18.0.21",
    "@types/react-dom": "^18.0.6",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "tsup": "^6.3.0",
    "typescript": "^4.8.4"
  },
  "peerDependencies": {
    "react": ">= 18.2.0",
    "react-dom": ">= 18.2.0"
  },
  "publishConfig": {
    "access": "public"
  }
}
