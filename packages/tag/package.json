{
  "name": "@jengaui/tag",
  "version": "0.2.1",
  "description": "Jenga UI Tag component in React",
  "keywords": [
    "tag",
    "react",
    "Jenga UI",
    "ui",
    "clickable",
    "tabbable"
  ],
  "homepage": "https://github.com/Outposthq/jengaui",
  "bugs": {
    "url": "https://github.com/Outposthq/jengaui/issues"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Outposthq/jengaui.git"
  },
  "license": "MIT",
  "sideEffects": false,
  "main": "./dist/index.js",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "files": [
    "dist/**"
  ],
  "scripts": {
    "build": "JSX=1 tsup src/index.ts --dts --minify --treeshake",
    "clean": "rm -rf .turbo && rm -rf node_modules && rm -rf dist",
    "dev": "tsup src/index.ts --minify --treeshake --format esm,cjs --watch --dts --external react",
    "lint": "TIMING=1 eslint src/**/*.ts* --fix"
  },
  "dependencies": {
    "@jengaui/button": "workspace:0.2.1",
    "@jengaui/core": "workspace:0.2.1",
    "@jengaui/layout": "workspace:0.2.1",
    "tastycss": "^0.9.1"
  },
  "devDependencies": {
    "@ant-design/icons": "^4.7.0",
    "@jengaui/tsconfig": "workspace:0.1.2",
    "@types/react": "^17.0.50",
    "@types/react-dom": "^17.0.17",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "tsup": "^6.2.3",
    "typescript": "^4.8.4"
  },
  "peerDependencies": {
    "react": ">= 18.2.0",
    "react-dom": ">= 18.2.0"
  },
  "publishConfig": {
    "access": "public"
  }
}
