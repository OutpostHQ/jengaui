    "lint": "TIMING=1 eslint src/**/*.ts* --fix"
  },
  "dependencies": {
    "tastycss": "^0.0.0-dev-20221117103514"
  },
  "devDependencies": {
    "@jengaui/tsconfig": "workspace:0.1.2",
    "@types/node": "18.11.9",
    "@types/react": "^17.0.50",
    "@types/react-dom": "^17.0.17",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "styled-components": "^5.3.6",
    "tsup": "^6.2.3",
    "typescript": "^4.8.4"
  },
  "peerDependencies": {
    "react": ">= 18.2.0",
    "react-dom": ">= 18.2.0",
    "styled-components": ">= 5.3.6"
