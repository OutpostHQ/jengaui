    "@jengaui/prism-code": "workspace:0.2.1",
    "@jengaui/toast": "workspace:0.2.1",
    "@jengaui/tooltip": "workspace:0.2.1",
    "clipboard-copy": "^4.0.1",
    "tastycss": "^0.13.0"
  },
  "devDependencies": {
    "@ant-design/icons": "^4.7.0",
    "@jengaui/tsconfig": "workspace:0.1.2",
